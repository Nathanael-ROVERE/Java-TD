package fr.umlv.morse;
import java.lang.StringBuilder;

public class Morse {
	
	//1
	public static void main(String[] args) {
		for (String item: args){
			System.out.print(item + " Stop. "));
		}
	}

	public static void main(String[] args) {
		for (String item: args){
			StringBuilder sb = new StringBuilder();
			sb.append(item + " Stop. ");
			System.out.print(sb.toString());
		}
	}
}
